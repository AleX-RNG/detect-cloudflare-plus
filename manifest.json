{
	"applications": {
		"gecko": {
			"id": "Detect-Cloudflare-PA@cm.org",
			"strict_min_version": "53.0"
		}
	},

	"background": {
		"scripts": [
			"background/functions.js",
			"background/globals.js",
			"background/runtime.js",
			"background/storage.js",
			"background/tabs.js",
			"background/webRequest.js"
		]
	},

	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"16": "icons/cf-orange-16.png",
			"32": "icons/cf-orange-32.png",
			"64": "icons/cf-orange-64.png"
		},
		"default_title": "Detect Cloudflare+",
		"default_popup": "popup/popup.html"
	},

	"description": "Displays the number of responses served by Cloudflare.",

	"icons": {
		"16": "icons/cf-orange-16.png",
		"32": "icons/cf-orange-32.png",
		"64": "icons/cf-orange-64.png"
	},

	"manifest_version": 2,
	"name": "Detect Cloudflare+",

	"options_ui": {
		"page": "options/options.html",
		"browser_style": true
	},

	"page_action": {
		"browser_style": true,
		"default_icon": {
			"16": "icons/cf-orange-16.png",
			"32": "icons/cf-orange-32.png",
			"64": "icons/cf-orange-64.png"
		},
		"default_title": "",
		"default_popup": "popup/popup.html"
	},

	"permissions": [
		"<all_urls>",
		"storage",
		"tabs",
		"webRequest"
	],

	"version": "0.14.0"
}
