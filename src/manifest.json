{
	"applications": {
		"gecko": {
			"id": "Detect-Cloudflare-PA@cm.org",
			"strict_min_version": "53.0"
		}
	},

	"background": {
		"scripts": [
			"background/classes.js",
			"background/globals.js",
			"background/runtime.js",
			"background/storage.js",
			"background/tabs.js",
			"background/webRequest.js"
		]
	},

	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"16": "icons/eye.svg",
			"32": "icons/eye.svg",
			"64": "icons/eye.svg"
		},
		"default_title": "True Sight",
		"default_popup": "popup/page.htm"
	},

	"description": "Reveals content delivery networks.",

	"icons": {
		"16": "icons/eye.svg",
		"32": "icons/eye.svg",
		"64": "icons/eye.svg"
	},

	"manifest_version": 2,
	"name": "True Sight",

	"options_ui": {
		"page": "options/page.htm",
		"browser_style": true
	},

	"page_action": {
		"browser_style": true,
		"default_icon": {
			"16": "icons/eye.svg",
			"32": "icons/eye.svg",
			"64": "icons/eye.svg"
		},
		"default_title": "",
		"default_popup": "popup/page.htm"
	},

	"permissions": [
		"<all_urls>",
		"storage",
		"tabs",
		"webRequest"
	],

	"version": "1.0.0"
}
